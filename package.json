{
    "name": "starter-package-gulp",
    "version": "1.0.0",
    "description": "Environment for promo-sites or multi-page applications development based on gulp",
    "main": "gulpfile.js",
    "author": "Itransition HTML developers Team",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://git.itransition.com/projects/WDFRONTEND/repos/front-end-academy/browse/starter_packages/SPVanilla_multipages_gulp"
    },
    "devDependencies": {
        "gulp": "github:gulpjs/gulp#4.0",
        "gulp-if": "2.0.2",
        "gulp-less": "3.3.2",
        "gulp-sass": "3.1.0",
        "gulp-sourcemaps": "2.6.0",
        "gulp-autoprefixer": "4.0.0",
        "gulp-remember": "0.3.1",
        "gulp-cssnano": "2.1.2",
        "gulp-file-include": "1.1.0",
        "gulp-useref": "3.1.2",
        "gulp-imagemin": "3.3.0",
        "gulp-newer": "1.3.0",
        "gulp-debug": "3.1.0",
        "babel-core": "7.0.0-alpha.19",
        "babel-preset-env": "1.6.0",
        "gulp-babel": "7.0.0",
        "gulp-eslint": "4.0.0",
        "gulp-jsmin": "0.1.5",
        "del": "3.0.0",
        "path": "0.12.7",
        "gulp-htmlmin": "3.0.0",
        "browser-sync": "2.18.13",
        "normalize.css": "7.0.0",
        "gulp-cached": "1.1.1",
        "gulp-concat": "2.6.1"
    },
    "browserslist": [
        "last 2 versions",
        "ie > 9"
    ],
    "eslintConfig": {
        "extends": [
            "eslint:recommended"
        ],
        "env": {
            "node": true,
            "browser": true,
            "es6": true,
            "jquery": true
        },
        "globals": {
            "jQuery": true,
            "$": true
        },
        "rules": {
            "quotes": [
                "error",
                "single"
            ],
            "indent": [
                "error",
                2
            ]
        }
    }
}
